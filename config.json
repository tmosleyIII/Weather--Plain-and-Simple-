{
	"author": "tmosleyiii@gmail.com", 
	"config_version": "2", 
	"description": "A Weather extension for Google Chrome built using Trigger.io", 
	"modules": {
		"activations": [
			{
				"all_frames": false, 
				"patterns": [
					"http://*/*", 
					"https://*/*"
				], 
				"run_at": "start", 
				"scripts": [
					"js/every-page.js"
				], 
				"styles": []
			}
		], 
		"background": {
			"files": [
				"js/background.js"
			]
		}, 
		"button": {
			"default_icon": "img/new_sun_icon.png", 
			"default_popup": "index.html", 
			"default_title": "Weather - Plain and Simple"
		}, 
		"contact": true, 
		"event": true, 
		"file": true, 
		"geolocation": true, 
		"is": true, 
		"logging": {
			"level": "INFO"
		}, 
		"message": true, 
		"notification": true, 
		"prefs": true, 
		"request": {
			"permissions": [
				"geolocation", 
				"http://www.google.com/*"
			]
		}, 
		"sms": true, 
		"tabs": true, 
		"tools": true
	}, 
	"name": "Weather - Plain and Simple", 
	"platform_version": "v1.3", 
	"version": "0.2.14"
}